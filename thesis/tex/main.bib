@article{doi:10.1111/j.1540-6261.2004.00680.x,
	Abstract = {ABSTRACT This study empirically evaluates the certification and value‐added roles of reputable venture capitalists (VCs). Using a novel sample of entrepreneurial start‐ups with multiple financing offers, I analyze financing offers made by competing VCs at the first professional round of start‐up funding, holding characteristics of the start‐up fixed. Offers made by VCs with a high reputation are three times more likely to be accepted, and high‐reputation VCs acquire start‐up equity at a 10--14\% discount. The evidence suggests that VCs' ``extra‐financial'' value may be more distinctive than their functionally equivalent financial capital. These extra‐financial services can have financial consequences.},
	Author = {David H. Hsu},
	Date-Added = {2018-04-19 08:42:51 +0000},
	Date-Modified = {2018-04-19 08:42:51 +0000},
	Doi = {10.1111/j.1540-6261.2004.00680.x},
	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1540-6261.2004.00680.x},
	Journal = {The Journal of Finance},
	Number = {4},
	Pages = {1805-1844},
	Title = {What Do Entrepreneurs Pay for Venture Capital Affiliation?},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1540-6261.2004.00680.x},
	Volume = {59},
